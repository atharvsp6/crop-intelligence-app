# Production Environment Variables for Railway
# Copy this to .env and fill in your production values

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=False
SECRET_KEY=your-production-secret-key-here

# MongoDB Atlas (Production Database)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/crop_intelligence_prod?retryWrites=true&w=majority

# JWT Configuration
JWT_SECRET_KEY=your-jwt-secret-key-here

# Google Gemini AI
GOOGLE_API_KEY=your-google-gemini-api-key-here

# Market Data APIs (Production Keys)
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key
COMMODITIES_API_KEY=your-commodities-api-key
MANDI_API_KEY=your-mandi-api-key

# CORS Configuration
FRONTEND_URL=https://your-vercel-app.vercel.app